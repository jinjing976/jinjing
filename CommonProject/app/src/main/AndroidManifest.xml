<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.loyangliu.android.commonproject">

<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
<activity android:label="代码覆盖率" android:name="codecov.mlf.CodecovActivity" ><intent-filter><action android:name="android.intent.action.MAIN" /><category android:name="android.intent.category.LAUNCHER" /></intent-filter></activity>
        <activity android:name="com.loyangliu.android.commonproject.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN.UNUSEFUL" />

                <category android:name="android.intent.category.LAUNCHER.UNUSEFUL" />
            </intent-filter>

            <intent-filter>
                <action android:name="com.loyangliu.android.commonproject.main.action" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
